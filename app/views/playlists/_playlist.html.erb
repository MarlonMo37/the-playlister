<% @playlists.each do |playlist| %>
     <h4><%= playlist.name %></h4>
    <h4>Tracks: <%= playlist.songs.length %></h4>
    <h4><%= link_to "View Playlist", playlist_path(playlist) %></h4>
    <%= link_to "Edit Playlist", edit_playlist_path(playlist) %>
    <%= button_to "Delete Playlist", playlist, method: :delete %>
    <br><br>
<% end %>